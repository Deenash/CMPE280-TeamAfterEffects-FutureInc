<script type="text/javascript">
		function toggleWhitePaperPopUp() {
			$("#blockscreen").show();
			$("#downloadWhite").show();

		}
		function closePopUp() {
			$("#downloadWhite").hide();
			$("#blockscreen").hide();

		}
		
		var chart1 = c3.generate({
			bindto: "#chart1",
		    data: {
		        columns: [
		            ['Mbps', ]
		        ],
		        type: 'gauge',
		        onclick: function (d, i) { console.log("onclick", d, i); },
		        onmouseover: function (d, i) { console.log("onmouseover", d, i); },
		        onmouseout: function (d, i) { console.log("onmouseout", d, i); }
		    },
		    gauge: {
		        label: {
		            format: function(value, ratio) {
		                return value;
		            },
		        },
		    min: 0, // 0 is default, //can handle negative min e.g. vacuum / voltage / current flow / rate of change
		    max: 100, // 100 is default
		    units: 'Mbps',
		    width: 90 // for adjusting arc thickness
		    },
		    color: {
		        pattern: ['#FF0000', '#F97600', '#F6C600', '#60B044'], // the three color levels for the percentage values.
		        threshold: {
		            unit: 'value', // percentage is default
		            max: 120, // 100 is default
		            values: [10, 30, 60, 70]
		        }
		    },
		    size: {
		        height: 180
		    }
		});
		
		var chart2 = c3.generate({
			bindto: "#chart2",
		    data: {
		        columns: [
		            ['Mbps', ]
		        ],
		        type: 'gauge',
		        onclick: function (d, i) { console.log("onclick", d, i); },
		        onmouseover: function (d, i) { console.log("onmouseover", d, i); },
		        onmouseout: function (d, i) { console.log("onmouseout", d, i); }
		    },
		    gauge: {
		        label: {
		            format: function(value, ratio) {
		                return value;
		            },
		        },
		    min: 0, // 0 is default, //can handle negative min e.g. vacuum / voltage / current flow / rate of change
		    max: 100, // 100 is default
		    units: 'Mbps',
		    width: 90 // for adjusting arc thickness
		    },
		    color: {
		        pattern: ['#FF0000', '#F97600', '#F6C600', '#60B044'], // the three color levels for the percentage values.
		        threshold: {
		            unit: 'value', // percentage is default
		            max: 120, // 100 is default
		            values: [10, 30, 60, 70]
		        }
		    },
		    size: {
		        height: 180
		    }
		});
		
		
		setInterval(function () {
			var defaultVal=60;
			var random = Math.floor(Math.random() * 6) + 1
		    chart1.load({
		        columns: [['data', defaultVal+random]]
		    });
		}, 100);
		
		setInterval(function () {
			var defaultVal=35;
			var random = Math.floor(Math.random() * 6) + 1
		    chart2.load({
		        columns: [['data', defaultVal+random]]
		    });
		}, 100);
		
		
		function submit()
	{
			
		var fname = $("#fname").val();
		var lname = $("#fname").val();
		var email =  $("#email").val();
		
		$.post("/downloadBroc",
    	{
	        fname: fname,
	        lname: lname,
	        email: email
   		 },
	    function(data, status){
	        console.log("Data: " + data + "\nStatus: " + status);
	        
	        document.getElementById("broc").download;
	        
	    });
	}
		
		
		
	</script>

